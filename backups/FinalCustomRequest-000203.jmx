import java.io.*;
import java.util.*;
import org.apache.jmeter.services.FileServer;

file = FileServer.getFileServer().getBaseDir() + (File.separator);
String file_path  = file+&quot;&quot;;
String edited_file_path = file_path.replace(&quot;\\&quot;, &quot;\\\\&quot;);
//edited_file_path = edited_file_path.replace(&quot;//&quot;,&quot;////&quot;);

props.put(&quot;fileName&quot;,edited_file_path);
